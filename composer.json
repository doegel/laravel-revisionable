{
  "name": "doegel/laravel-revisionable",
  "description": "drop-in module to keep revisions of Eloquent models",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Felix Rudat",
      "email": "frudat@doegel.de"
    }
  ],
  "require": {
    "php" : ">=7.1",
    "illuminate/support": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
    "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
    "illuminate/database": "~5.4.0|~5.5.0|~5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "^5.7|6.2|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Revisionable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revisionable\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Revisionable\\RevisionableServiceProvider"
      ]
    }
  }
}
