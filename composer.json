{
  "name": "doegel/laravel-revisionable",
  "description": "drop-in module to keep revisions of Eloquent models",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Felix Rudat",
      "email": "frudat@doegel.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/contracts": "^5.6",
    "illuminate/support": "^5.6",
    "jenssegers/mongodb": "^3.4"
  },
  "require-dev": {
    "orchestra/testbench": "^3.6",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Revisionable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revisionable\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Revisionable\\RevisionableServiceProvider"
      ]
    }
  }
}
